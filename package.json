{
  "name": "giggity2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "formik": "^1.3.2",
    "formik-material-ui": "^0.0.15",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
