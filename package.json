    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "backend/"
    ]
  },
  "proxy": "http://localhost:9000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
